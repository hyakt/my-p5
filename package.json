  },
  "homepage": "https://github.com/hyakt/p5#readme",
  "devDependencies": {
    "@types/p5": "^1.3.0",
    "parcel": "next",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "p5": "^1.4.0",
    "reset-css": "^5.0.1"
  }
}
